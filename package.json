{
  "name": "sp-editor",
  "version": "3.27.0",
  "description": "sp-project version 3",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Jared Parkinson",
    "email": "jared@parkinson.im"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "yarn run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "yarn run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "yarn run postinstall:electron && yarn run electron:tsc && ng build",
    "build:dev": "yarn version --minor && yarn run build -- -c dev",
    "build:prod": "yarn version --patch && yarn run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "yarn run postinstall:web && ng serve -o",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && yarn run electron:tsc && electron . --serve",
    "electron:local": "yarn run build:prod && electron .",
    "electron:linux": "yarn run build:prod && npx electron-builder build --linux",
    "electron:windows": "yarn run build:prod && npx electron-builder build --windows",
    "electron:windows:dev": "yarn run build:dev && npx electron-builder build --windows",
    "electron:mac": "yarn run build:prod && npx electron-builder build --mac",
    "test": "yarn run postinstall:web && ng test",
    "e2e": "yarn run postinstall:web && ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/material": "^6.4.7",
    "@angular/pwa": "^0.6.8",
    "@angular/service-worker": "^6.1.1",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@types/graceful-fs": "^4.1.2",
    "graceful-fs": "^4.1.11",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.2",
    "nodejs": "0.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.2.2",
    "tslint-config-prettier": "^1.14.0",
    "tslint-microsoft-contrib": "^5.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.1.2",
    "@angular/common": "6.1.2",
    "@angular/compiler": "6.1.2",
    "@angular/compiler-cli": "6.1.2",
    "@angular/core": "6.1.2",
    "@angular/forms": "6.1.2",
    "@angular/http": "6.1.2",
    "@angular/language-service": "6.1.2",
    "@angular/platform-browser": "6.1.2",
    "@angular/platform-browser-dynamic": "6.1.2",
    "@angular/router": "6.1.2",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/angular": "^1.6.49",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/jquery": "^3.3.5",
    "@types/node": "10.5.2",
    "codelyzer": "4.4.2",
    "core-js": "2.5.7",
    "electron": "2.0.5",
    "electron-builder": "^20.27.1",
    "electron-reload": "1.2.5",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.3.2",
    "ts-node": "6.0.3",
    "tslint": "^5.10.0",
    "typescript": ">=2.7.2 <2.10.0",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.1.0",
    "zone.js": "0.8.26"
  }
}
